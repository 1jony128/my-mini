    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.20.1",
    "react-syntax-highlighter": "^15.6.6",
